<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.covid19app.MainpageMemberController">
   <children>
      <StackPane prefHeight="150.0" prefWidth="200.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="250.0" style="-fx-background-color: #4F8EDB;">
                     <children>
                        <Button fx:id="newsbtn" mnemonicParsing="false" onMouseClicked="#newsclicked" prefHeight="50.0" prefWidth="300.0" style="-fx-background-color: transparent; -fx-background-radius: 100 0 0 100; -fx-font-family: Quicksand; -fx-font-size: 26; -fx-font-weight: bold; -fx-text-fill: white;" text="News" textFill="WHITE">
                           <VBox.margin>
                              <Insets top="150.0" />
                           </VBox.margin>
                           <effect>
                              <Glow />
                           </effect>
                        </Button>
                        <Button fx:id="infomationbtn" mnemonicParsing="false" onMouseClicked="#informationclicked" prefHeight="50.0" prefWidth="300.0" style="-fx-background-color: transparent; -fx-background-radius: 100 0 0 100; -fx-font-family: Quicksand; -fx-font-size: 26; -fx-font-weight: bold; -fx-text-fill: white;" text="Information" textFill="WHITE">
                           <VBox.margin>
                              <Insets top="40.0" />
                           </VBox.margin>
                           <effect>
                              <Glow />
                           </effect>
                        </Button>
                        <Button fx:id="treatmentbtn" mnemonicParsing="false" onMouseClicked="#treatmentclicked" prefHeight="50.0" prefWidth="300.0" style="-fx-background-color: transparent; -fx-background-radius: 100 0 0 100; -fx-font-family: Quicksand; -fx-font-size: 26; -fx-font-weight: bold; -fx-text-fill: white;" text="Treatment" textFill="WHITE">
                           <VBox.margin>
                              <Insets top="40.0" />
                           </VBox.margin>
                           <effect>
                              <Glow />
                           </effect>
                        </Button>
                        <Button fx:id="signoutbtn" mnemonicParsing="false" onMouseClicked="#signoutclicked" prefHeight="50.0" prefWidth="300.0" style="-fx-background-color: transparent; -fx-background-radius: 100 0 0 100; -fx-font-family: Quicksand; -fx-font-size: 26; -fx-font-weight: bold; -fx-text-fill: white;" text="Sign out" textFill="WHITE">
                           <VBox.margin>
                              <Insets top="40.0" />
                           </VBox.margin>
                           <effect>
                              <Glow />
                           </effect>
                        </Button>
                     </children>
                  </VBox>
                  <VBox prefHeight="200.0" prefWidth="100.0" style="-fx-background-color: white;" HBox.hgrow="ALWAYS">
                     <children>
                        <VBox prefHeight="50.0" prefWidth="100.0" />
                        <VBox alignment="CENTER" prefHeight="150.0" prefWidth="100.0" style="-fx-background-color: #9cc9ff;" />
                        <VBox fx:id="content" prefHeight="200.0" prefWidth="100.0" VBox.vgrow="ALWAYS" />
                     </children></VBox>
               </children>
            </HBox>
         </children>
         <effect>
            <DropShadow />
         </effect>
      </StackPane>
   </children>
</AnchorPane>
